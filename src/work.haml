%html
  %head
    %title Found Niya
    %link(rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css" )
    %link(rel="stylesheet" href="styles/page_content.css" )
    %link(rel="stylesheet" href="styles/topnav_styles.css" )
    %link(rel="stylesheet" href="styles/footnav_styles.css" )
  %body
    -#the nav and logo
    .container.top-nav
      .row
        .col-xs-12.brand
          %a(href="index.html")
            %img(src="images/header.png" )
      .row
        .col-xs-12.topnav
          %ul.nav
            %li(role="presentation")
              %a(href="#")
                %img(src="images/header.png" )
                %span Work
            %li(role="presentation")
              %a(href="about.html")
                %img(src="images/header.png" )
                %span About
            %li(role="presentation")
              %a(href="#")
                %img(src="images/header.png" )
                %span Blog
            %li(role="presentation")
              %a(href="contact.html")
                %img(src="images/header.png" )
                %span Contact

    -#the main body
    .container.home
      .row
        .col-xs-12
          %ul.nav.nav-tabs#myTab(role="tablist")
            %li.active(role="presentation")
              %a(href="#web" aria-controls="home" role="tab" data-toggle="tab" )Web Design
            %li(role="presentation" )
              %a(href="#graphics" aria-controls="profile" role="tab" data-toggle="tab") Graphic Design
            %li(role="presentation" )
              %a(href="#phot" aria-controls="messages" role="tab" data-toggle="tab") Photo Editing

          .tab-content
            .tab-pane.active#home(role="tabpanel")
            .tab-pane.active#profile(role="tabpanel")
            .tab-pane.active#messages(role="tabpanel")
          %table(align="center" )
            %thead
            %tbody
              %tr
                %td
                  %a(href="#" )
                    %img(src="images/juliebees/winterwakeup.jpg" )
                %td
                  %h2 Julie Bee's
                  %p Build Website.
              %tr
                %td
                  %h2 Zebrablinds
                  %p Build Website.

    -#the footer
    .container.foot-nav
      .row
        .col-xs-12.
          %ul.nav
            %li(role="presentation")
              %a(href="work.html")
                %span Work
            %li(role="presentation")
              %a(href="about.html")
                %span About
            %li(role="presentation")
              %a(href="index.html" )
                %img(src="images/fn.png" )
            %li(role="presentation")
              %a(href="#")
                %span Blog
            %li(role="presentation")
              %a(href="contact.html")
                %span Contact

    %script(src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js")
    %script(src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js")
